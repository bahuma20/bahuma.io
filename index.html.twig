                            <a class="btn" href="">Learn More</a>
                            #}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- About End -->
        
        
        <!-- Service Start -->
        <div class="service" id="skills">
            <div class="container">
                <div class="section-header text-center wow zoomIn" data-wow-delay="0.1s">
                    <p>Was mache ich</p>
                    <h2>Kenntnisse und Fähigkeiten</h2>
                </div>
                <div class="row">
                    {% for skill in resume.skills %}
                    <div class="col-lg-6 wow fadeInUp" data-wow-delay="{{ loop.index0 * 0.2 }}s">
                        <div class="service-item">
                            <div class="service-icon">
                                <i class="{{ skill.icon }}"></i>
                            </div>
                            <div class="service-text">
                                <h3>{{ skill.name }}</h3>
                                <p>
                                    {% for keyword in skill.keywords %}<span>{{ keyword }}</span>{% if not loop.last %}, {% endif %}{% endfor %}
                                </p>
                            </div>
                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <!-- Service End -->
        
        
         <!-- Experience Start -->
        <div class="experience" id="experience">
            <div class="container">
                <header class="section-header text-center wow zoomIn" data-wow-delay="0.1s">
                    <p>Mein Lebenslauf</p>
                    <h2>Berufserfahrung</h2>
                </header>
                <div class="timeline">
                    {% for workItem in resume.work %}
                    <div class="timeline-item wow {% if loop.index is odd %}left slideInLeft{% else %}right slideInRight{% endif %}" data-wow-delay="0.1s">
                        <div class="timeline-text">
                            <div class="timeline-date">{{ workItem.startDate|date('M Y') }} - {% if workItem.endDate %}{{ workItem.endDate|date('M Y') }}{% else %}jetzt{% endif %}</div>
                            <h2>{{ workItem.position }}</h2>
                            <h4><a href="{{ workItem.url }}" target="_blank">{{ workItem.company }}</a>, {{ workItem.location }}</h4>
                            <p>{{ workItem.summary }}</p>
                            {% if workItem.highlights %}
                            <p>Unter anderem:</p>
                            <ul>
                                {% for highlight in workItem.highlights %}
                                    <li>{{ highlight }}</li>
                                {% endfor %}
                            </ul>
                            {% endif %}
                        </div>
                    </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <!-- Job Experience End -->


        <!-- Ausbildung Start -->
        <div class="experience" id="ausbildung">
            <div class="container">
                <header class="section-header text-center wow zoomIn" data-wow-delay="0.1s">
                    <p>Mein Werdegang</p>
                    <h2>Ausbildung</h2>
                </header>
                <div class="timeline">
                    {% for educationItem in resume.education %}
                        <div class="timeline-item wow {% if loop.index is odd %}left slideInLeft{% else %}right slideInRight{% endif %}" data-wow-delay="0.1s">
                            <div class="timeline-text">
                                <div class="timeline-date">{{ educationItem.startDate|date('M Y') }} - {% if educationItem.endDate %}{{ educationItem.endDate|date('M Y') }}{% else %}jetzt{% endif %}</div>
                                <h2>{{ educationItem.studyType }}</h2>
                                <h4><a href="{{ educationItem.url }}" target="_blank">{{ educationItem.institution }}</a></h4>
                                <p>{{ educationItem.area }}</p>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <!-- Ausbildung End -->

        <!-- Zertifikate Start -->
        <div class="service" id="zertifikate">
            <div class="container">
                <div class="section-header text-center wow zoomIn" data-wow-delay="0.1s">
                    <p>Was kann ich</p>
                    <h2>Zertifikate</h2>
                </div>
                <div class="row">
                    {% for certificate in resume.certificates %}
                        <div class="col-lg-6 wow fadeInUp" data-wow-delay="{{ loop.index0 * 0.2 }}s">
                            <div class="service-item">
                                <div class="service-icon">
                                    {% if certificate.badge %}
                                        <img src="{{ certificate.badge }}" width="198" height="198" alt="{{ certificate.name }} badge">
                                    {% else %}
                                        <i class="fa fa-award"></i>
                                    {% endif %}
                                </div>
                                <div class="service-text">
                                    <h3><a href="{{ certificate.url }}" target="_blank">{{ certificate.name }}</a></h3>
                                    <h5>{{ certificate.issuer }}</h5>
                                    <p>
                                        {{ certificate.date|date('d.m.Y') }}
                                    </p>
                                </div>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <!-- Zertifikate End -->

        <!-- Interessen Start -->
        <div class="banner interests wow zoomIn" data-wow-delay="0.1s" id="interessen">
            <div class="container">
                <div class="section-header text-center">
                    <p>Was es sonst noch zu wissen gibt</p>
                    <h2>Meine Interessen</h2>
                </div>
                <div class="container banner-text interest-items">
                    {% for interest in resume.interests %}
                        <div class="interest-item">
                            {% if interest.icon %}
                            <div class="interest-item-icon"><i class="{{ interest.icon }}"></i></div>
                            {% endif %}
                            <div class="interest-item-text">
                                <div class="interest-item-name">{{ interest.name }}</div>
                                {% if interest.keywords %}
                                <div class="interest-item-keywords">
                                {% for keyword in interest.keywords %}{{ keyword }}{% if not loop.last %}, {% endif %}{% endfor %}
                                </div>
                                {% endif %}
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <!-- Interessen End -->

        <!-- Ehrenamt Start -->
        <div class="experience" id="ehrenamt">
            <div class="container">
                <header class="section-header text-center wow zoomIn" data-wow-delay="0.1s">
                    <p>Dafür engagiere ich mich</p>
                    <h2>Ehrenamt</h2>
                </header>
                <div class="timeline">
                    {% for volunteerItem in resume.volunteer %}
                        <div class="timeline-item wow {% if loop.index is odd %}left slideInLeft{% else %}right slideInRight{% endif %}" data-wow-delay="0.1s">
