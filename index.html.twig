                                <h4><a href="{{ volunteerItem.url }}" target="_blank">{{ volunteerItem.organization }}</a></h4>
                                <p>{{ volunteerItem.summary }}</p>
                                {% if volunteerItem.highlights %}
                                    <p>Unter anderem:</p>
                                    <ul>
                                        {% for highlight in volunteerItem.highlights %}
                                            <li>{{ highlight }}</li>
                                        {% endfor %}
                                    </ul>
                                {% endif %}
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <!-- Ehrenamt End -->


