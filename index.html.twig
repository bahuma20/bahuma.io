<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>Max Bachhuber</title>
        <meta content="width=device-width, initial-scale=1.0" name="viewport">
        <meta name="description" content="{{ resume.basics.label }}">

        <!-- Favicon -->
        <link href="favicon.ico" rel="icon">

        <!-- Google Font -->
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet">

        <!-- CSS Libraries -->
        <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet">
        <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
        <link href="lib/animate/animate.min.css" rel="stylesheet">
        <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
        <link href="lib/lightbox/css/lightbox.min.css" rel="stylesheet">

        <!-- Template Stylesheet -->
        <link href="css/style.css" rel="stylesheet">

        <!-- Pinterest verify -->
        <meta name="p:domain_verify" content="160395556e4ddb17721c8b5da726fa96"/>

        <!-- Flattr verify -->
        <meta name="flattr:id" content="m1xjz6">

        <script type="application/ld+json">
            {
                "@context": "http://schema.org/",
                "@type": "Person",
                "name": "{{ resume.basics.name }}",
                "address": {
                    "@context": "http://schema.org/",
                    "@type": "PostalAddress",
                    "addressLocality": "{{ resume.basics.location.city }}",
                    "addressRegion": "{{ resume.basics.location.region }}",
                    "postalCode": "{{ resume.basics.location.postalCode }}",
                    "streetAddress": "{{ resume.basics.location.address }}",
                    "addressCountry": "{{ resume.basics.location.countryCode }}"
                },
                "memberOf": {
                    "@context": "http://schema.org/",
                    "@type": "Organization",
                    "name": "{{ resume.work.0.company }}",
                    "url": "{{ resume.work.0.url }}"
                },
                "jobTitle": "{{ resume.work.0.position }}",
                "telephone": "{{ resume.basics.phone }}",
                "email": "{{ resume.basics.email }}",
                "familyName": "{{ resume.basics.name|split(' ').0 }}",
                "givenName": "{{ resume.basics.name|split(' ').1 }}",
                "gender": "male",
                "url": "{{ resume.basics.url }}",
                "birthDate": "{{ resume.basics.birth.date }}",
                "knowsLanguage": [
                    {% for language in resume.languages %}
                    "{{ language.language }}"{% if not loop.last %},{% endif %}
                    {% endfor %}
                ],
                "nationality": "{{ resume.basics.location.countryCode }}",
                "image": "{{ resume.basics.url }}"
            }
        </script>
    </head>

    <body data-spy="scroll" data-target=".navbar" data-offset="51">
        <!-- Nav Bar Start -->
        <div class="navbar navbar-expand-lg bg-light navbar-light">
            <div class="container-fluid">
{#                <a href="index.html" class="navbar-brand">Max Bachhuber</a>#}
                <button type="button" class="navbar-toggler" data-toggle="collapse" data-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse justify-content-between" id="navbarCollapse">
                    <div class="navbar-nav ml-auto mr-auto">
                        <a href="#home" class="nav-item nav-link active">Home</a>
                        <a href="#about" class="nav-item nav-link">Über mich</a>
                        <a href="#skills" class="nav-item nav-link">Skills</a>
                        <a href="#experience" class="nav-item nav-link">Berufserfahrung</a>
                        <a href="#ausbildung" class="nav-item nav-link">Ausbildung</a>
                        <a href="#zertifikate" class="nav-item nav-link">Zertifikate</a>
                        <a href="#interessen" class="nav-item nav-link">Interessen</a>
                        <a href="#ehrenamt" class="nav-item nav-link">Ehrenamt</a>
                    </div>
                </div>
            </div>
        </div>
        <!-- Nav Bar End -->


        <!-- Hero Start -->
        <div class="hero" id="home">
            <div class="container-fluid">
                <div class="row align-items-center">
                    <div class="col-sm-12 col-md-6">
                        <div class="hero-content">
                            <div class="hero-text">
                                <p>Ich bin</p>
                                <h1>{{ resume.basics.name }}</h1>
                                <h2></h2>
                                <div class="typed-text">{{ resume.basics.label }}</div>
                            </div>
                            {#
                            <div class="hero-btn">
                                <a class="btn" href="">Hire Me</a>
                                <a class="btn" href="">Contact Me</a>
                            </div>
                            #}
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6 d-none d-md-block">
                        <div class="hero-image">
                            <img src="img/hero.png" alt="Hero Image">
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Hero End -->


        <!-- About Start -->
        <div class="about wow fadeInUp" data-wow-delay="0.1s" id="about">
            <div class="container-fluid">
                <div class="row align-items-center">
                    <div class="col-lg-6">
                        <div class="about-img">
                            <img src="img/about.jpg" alt="Image">
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="about-content">
                            <div class="section-header text-left">
                                <p>Wer bin ich</p>
                                <h2>Über mich</h2>
                            </div>
                            <div class="about-text">
                                {% set summaryParagraphs = resume.basics.summary|split('\n') %}
                                {% for line in summaryParagraphs %}
                                <p>
                                    {{ line }}
                                </p>
                                {% endfor %}
                            </div>
                            <div class="about-details">
                                <p><i class="fa fa-envelope"></i> <a href="mailto:{{ resume.basics.email }}">{{ resume.basics.email }}</a></p>
                                <p><i class="fa fa-phone"></i> <a href="tel:{{ resume.basics.phone }}">{{ resume.basics.phone }}</a></p>
                                <p class="about-location"><i class="fa fa-map-marker"></i> {{ resume.basics.location.address }}<br>{{ resume.basics.location.postalCode }} {{ resume.basics.location.city }}<br>Deutschland</p>
                                {% for profile in resume.basics.profiles %}
                                <p><i class="{{ profile.icon }}"></i> <a href="{{ profile.url }}" rel="me" target="_blank" title="{{ profile.network }}">{{ profile.username }}</a></p>
                                {% endfor %}
                            </div>
                            {# <div class="skills">
                                <div class="skill-name">
                                    <p>Web Design</p><p>85%</p>
                                </div>
                                <div class="progress">
                                    <div class="progress-bar" role="progressbar" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100"></div>
                                </div>
